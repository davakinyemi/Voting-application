<div class="list-group" style="margin: 0px;">
    <% if(polls) {%>
        <% for(var i = 0; i < polls.length; i++) { %>
            <a href="/poll?num=<%= polls[i].pollid %>" class="list-group-item"><%= polls[i].title %></a>
        <% } %>
    <% } %>

    <% if(nopolls) {%>
    <p><%= nopolls %></p>
    <% } %>
</div>